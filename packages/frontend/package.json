  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@plantarium/generator": "file:../generator",
    "@plantarium/helpers": "file:../helpers",
    "@plantarium/nodes": "file:../nodes",
    "@plantarium/nodesystem": "file:../nodesystem",
    "@plantarium/renderer": "file:../renderer",
    "@plantarium/types": "file:../types",
    "ogl-typescript": "^0.1.20",
    "resize-observer-polyfill": "^1.5.1",
    "settings-ui": "^1.3.2"
  }
