{
  "name": "@plantarium/nodesystem",
  "version": "1.0.2",
  "main": "src/index.ts",
  "source": "src/index.ts",
  "keywords": [
    "procedural",
    "ui",
    "math",
    "nodes"
  ],
  "license": "MIT",
  "styles": "./public/dist/index.esm.css",
  "scripts": {
    "clean": "rimraf coverage .nyc_output public-test public/dist .eslintcache",
    "build": "node build.js && pnpm build:types",
    "build:test": "pnpm build && cp -r public public-test && nyc instrument public/dist public-test/dist",
    "build:types": "tsc",
    "dev": "node build.js --dev",
    "serve": "sirv public-test -p 8084",
    "test": "pnpm build:test && run-p --race serve cy:run",
    "cy:run": "wait-on http-get://localhost:8084 && pnpm cypress run",
    "cy:open": "wait-on http-get://localhost:8084 && pnpm cypress open",
    "lint": "eslint src/**/*.ts --cache"
  },
  "dependencies": {
    "@plantarium/helpers": "link:../helpers",
    "@plantarium/ui": "link:../ui"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@es-pack/esbuild-sass-plugin": "^2.0.1",
    "@tsconfig/svelte": "^2.0.1",
    "cypress": "^8.7.0",
    "esbuild": "^0.12.29",
    "esbuild-svelte": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "sirv-cli": "^1.0.14",
    "source-map-support": "^0.5.21",
    "svelte": "^3.44.2",
    "svelte-preprocess": "^4.9.8",
    "svelte-preprocess-esbuild": "^2.0.0",
    "typescript": "^4.5.2",
    "wait-on": "^6.0.0"
  }
}
