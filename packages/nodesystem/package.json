{
  "name": "@plantarium/nodesystem",
  "version": "1.0.2",
  "main": "src/index.ts",
  "source": "src/index.ts",
  "keywords": [
    "procedural",
    "ui",
    "math",
    "nodes"
  ],
  "license": "MIT",
  "styles": "./public/dist/index.css",
  "scripts": {
    "clean": "rimraf coverage .nyc_output public-test public/dist .eslintcache",
    "build": "node build.mjs",
    "build:test": "pnpm build && cp -r public public-test && nyc instrument public/dist public-test/dist",
    "dev": "node build.js --dev",
    "serve": "sirv public-test -p 8084",
    "test": "pnpm build:test && run-p --race serve test-unit test-int:run",
    "coverage": "c8 pnpm test",
    "test-unit": "wait-on http-get://localhost:8084 && playwright-test tests/units --runner uvu",
    "test-int:run": "wait-on http-get://localhost:8084 && playwright test",
    "test-int:open": "wait-on http-get://localhost:8084 && PWDEBUG=1 playwright test",
    "lint": "eslint src/**/*.ts --cache"
  },
  "dependencies": {
    "@plantarium/helpers": "link:../helpers",
    "@plantarium/ui": "link:../ui"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.0",
    "c8": "^7.11.2",
    "cypress": "^9.6.1",
    "esbuild": "^0.14.39",
    "esbuild-sass-plugin": "^2.2.6",
    "esbuild-svelte": "^0.7.0",
    "loadr": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.22.0",
    "playwright-test": "^7.4.0",
    "sirv-cli": "^2.0.2",
    "source-map-support": "^0.5.21",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-preprocess-esbuild": "^2.0.0",
    "tsm": "^2.2.1",
    "typescript": "^4.6.4",
    "uvu": "^0.5.3",
    "wait-on": "^6.0.1"
  }
}
