{
  "name": "@plantarium/ui",
  "version": "1.0.0",
  "module": "src/lib/index.ts",
  "types": "src/lib/index.ts",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run package",
    "postpublish": "rm -r ./package",
    "dev": "vite dev --host",
    "build": "vite build",
    "package": "svelte-kit package && pnpm i --filter=./package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . --cache",
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint . --fix",
    "clean": "rimraf coverage .nyc_output public/dist .eslintcache"
  },
  "devDependencies": {
    "@plantarium/helpers": "workspace:*",
    "@plantarium/theme": "workspace:*",
    "@plantarium/types": "workspace:*",
    "@sveltejs/adapter-static": "1.0.6",
    "@sveltejs/kit": "1.3.10",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@testing-library/svelte": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vitest/ui": "^0.28.4",
    "@yr/monotone-cubic-spline": "1.0.3",
    "c8": "7.12.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-svelte3": "4.0.0",
    "file-saver": "2.0.5",
    "highlight.js": "11.7.0",
    "jsdom": "21.1.0",
    "prettier": "2.8.3",
    "prettier-plugin-svelte": "2.9.0",
    "sass": "1.62.0",
    "svelte": "^3.55.1",
    "svelte-check": "3.0.3",
    "svelte-highlight": "7.1.2",
    "svelte-preprocess": "5.0.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "uvu": "0.5.6",
    "vite": "4.1.1",
    "vitest": "^0.28.4"
  },
  "type": "module",
  "jest": {
    "testRegex": "./tests/.*.ts$"
  },
  "dependencies": {
    "@plantarium/ui": "link:",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "css-vars-from-json": "1.1.0",
    "nanoid": "4.0.1",
    "ogl-typescript": "0.1.40",
    "stacktrace-parser": "0.1.10",
    "svelte-json-tree": "1.0.0"
  }
}
