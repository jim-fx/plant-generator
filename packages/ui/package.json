{
  "name": "@plantarium/ui",
  "version": "1.0.0",
  "main": "src/lib/index.ts",
  "source": "src/lib/index.ts",
  "svelte": "src/lib/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . --cache",
    "coverage": "c8 vitest --run",
    "test": "vitest",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint . --fix",
    "clean": "rimraf coverage .nyc_output public/dist .eslintcache"
  },
  "devDependencies": {
    "@plantarium/helpers": "link:../helpers",
    "@plantarium/theme": "link:../theme",
    "@sveltejs/adapter-static": "1.0.0-next.30",
    "@sveltejs/kit": "1.0.0-next.330",
    "@testing-library/svelte": "3.1.1",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "@yr/monotone-cubic-spline": "1.0.3",
    "c8": "7.11.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "file-saver": "2.0.5",
    "highlight.js": "11.5.1",
    "jsdom": "19.0.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "sass": "1.51.0",
    "shortid": "2.2.16",
    "svelte": "3.48.0",
    "svelte-check": "2.7.1",
    "svelte-highlight": "6.0.1",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "uvu": "0.5.3",
    "vite": "2.9.9",
    "vitest": "0.12.6"
  },
  "type": "module",
  "jest": {
    "testRegex": "./tests/.*.ts$"
  },
  "dependencies": {
    "ogl-typescript": "^0.1.40"
  }
}
