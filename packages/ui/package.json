    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "highlight.js": "^11.3.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.44.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-highlight": "^5.1.3",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "@sveltejs/adapter-static": "^1.0.0-next.21",
