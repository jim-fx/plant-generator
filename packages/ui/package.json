{
  "name": "@plantarium/ui",
  "version": "1.0.0",
  "module": "src/lib/index.ts",
  "types": "src/lib/index.ts",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run package",
    "postpublish": "rm -r ./package",
    "dev": "vite dev --host",
    "build": "vite build",
    "package": "svelte-kit package && pnpm i --filter=./package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . --cache",
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint . --fix",
    "clean": "rimraf coverage .nyc_output public/dist .eslintcache"
  },
  "devDependencies": {
    "@plantarium/helpers": "workspace:*",
    "@plantarium/theme": "workspace:*",
    "@plantarium/types": "workspace:*",
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "1.15.5",
    "@sveltejs/vite-plugin-svelte": "^2.2.0",
    "@testing-library/svelte": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitest/ui": "^0.30.1",
    "@yr/monotone-cubic-spline": "1.0.3",
    "c8": "7.13.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "file-saver": "2.0.5",
    "highlight.js": "11.7.0",
    "jsdom": "21.1.2",
    "prettier": "2.8.7",
    "prettier-plugin-svelte": "2.10.0",
    "sass": "1.62.0",
    "svelte": "^3.59.1",
    "svelte-check": "3.2.0",
    "svelte-highlight": "7.2.1",
    "svelte-preprocess": "5.0.3",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "uvu": "0.5.6",
    "vite": "4.3.0-beta.2",
    "vitest": "^0.30.1"
  },
  "type": "module",
  "jest": {
    "testRegex": "./tests/.*.ts$"
  },
  "pnpm": {
    "overrides": {
      "vite": "4.3.0-beta.2"
    }
  },
  "dependencies": {
    "@plantarium/ui": "link:",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "css-vars-from-json": "1.1.0",
    "nanoid": "4.0.2",
    "ogl-typescript": "0.1.40",
    "stacktrace-parser": "0.1.10",
    "svelte-json-tree": "1.0.0"
  }
}
