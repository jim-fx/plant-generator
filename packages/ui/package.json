{
  "name": "@plantarium/ui",
  "version": "1.0.0",
  "main": "src/lib/index.ts",
  "source": "src/lib/index.ts",
  "svelte": "src/lib/index.ts",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run package",
    "postpublish": "rm -r ./package",
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "package": "svelte-kit package && pnpm i --filter=./package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . --cache",
    "coverage": "c8 vitest --run",
    "test": "vitest",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint . --fix",
    "clean": "rimraf coverage .nyc_output public/dist .eslintcache"
  },
  "devDependencies": {
    "@plantarium/helpers": "workspace:*",
    "@plantarium/theme": "workspace:*",
    "@sveltejs/adapter-static": "1.0.0-next.34",
    "@sveltejs/kit": "1.0.0-next.350",
    "@testing-library/svelte": "3.1.3",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "@yr/monotone-cubic-spline": "1.0.3",
    "c8": "7.11.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "file-saver": "2.0.5",
    "highlight.js": "11.5.1",
    "jsdom": "19.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "sass": "1.52.3",
    "shortid": "2.2.16",
    "svelte": "3.48.0",
    "svelte-check": "2.7.2",
    "svelte-highlight": "6.1.0",
    "svelte-preprocess": "4.10.7",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "uvu": "0.5.3",
    "vite": "2.9.12",
    "vitest": "0.15.1"
  },
  "type": "module",
  "jest": {
    "testRegex": "./tests/.*.ts$"
  },
  "dependencies": {
    "css-vars-from-json": "^1.1.0",
    "ogl-typescript": "^0.1.40",
    "stacktrace-parser": "^0.1.10"
  }
}
