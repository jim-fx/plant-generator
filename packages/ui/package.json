{
  "name": "@plantarium/ui",
  "version": "1.0.0",
  "module": "src/lib/index.ts",
  "types": "src/lib/index.ts",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run package",
    "postpublish": "rm -r ./package",
    "dev": "vite dev --host",
    "build": "vite build",
    "package": "svelte-kit package && pnpm i --filter=./package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . --cache",
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint . --fix",
    "clean": "rimraf coverage .nyc_output public/dist .eslintcache"
  },
  "devDependencies": {
    "@plantarium/helpers": "workspace:*",
    "@plantarium/theme": "workspace:*",
    "@sveltejs/adapter-static": "1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.405",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@testing-library/svelte": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@vitest/ui": "0.22.0",
    "@yr/monotone-cubic-spline": "1.0.3",
    "c8": "7.12.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "file-saver": "2.0.5",
    "highlight.js": "11.6.0",
    "jsdom": "^20.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "sass": "1.54.4",
    "shortid": "2.2.16",
    "svelte": "^3.49.0",
    "svelte-check": "2.8.0",
    "svelte-highlight": "6.2.1",
    "svelte-preprocess": "4.10.7",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "uvu": "0.5.6",
    "vite": "3.0.7",
    "vitest": "^0.21.1"
  },
  "type": "module",
  "jest": {
    "testRegex": "./tests/.*.ts$"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "css-vars-from-json": "^1.1.0",
    "ogl-typescript": "^0.1.40",
    "stacktrace-parser": "^0.1.10"
  }
}
