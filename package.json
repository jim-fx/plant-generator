{
  "name": "@plantarium/root",
  "version": "0.0.1",
  "published": "true",
  "scripts": {
    "frontend:dev": "pnpm --filter=frontend dev",
    "frontend:build": "pnpm --filter=frontend build",
    "backend:dev": "pnpm --filter=backend dev",
    "backend:start": "pnpm --filter=backend start",
    "backend:build": "pnpm --filter=backend build",
    "build": "pnpm m build",
    "dev": "pnpm dev -r --filter !@plantarium/ui",
    "start": "pnpm build && pnpm backend:start",
    "test": "pnpm m test",
    "coverage": "pnpm m coverage && node bin/combine_coverage.mjs && nyc report",
    "clean": "pnpm m clean && rimraf coverage .nyc_output"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "typescript": "^4.3.5"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jim-fx/plantarium.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jim-fx/plantarium/issues"
  },
  "homepage": "https://github.com/jim-fx/plantarium#readme",
  "description": ""
}
