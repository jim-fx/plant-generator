{
  "name": "plantarium",
  "version": "0.0.7",
  "description": "Tool for the procedural generation of 3D plants.",
  "scripts": {
    "dev": "npm run client:watch",
    "start": "npm run server:start",
    "build": "npm run client:build",
    "client:watch": "rollup -c -w ",
    "client:build": "NODE_ENV=production rollup -c && mv stats.html build/stats.html",
    "server:start": "node server/server.js",
    "server:watch": "nodemon --watch server server/server.js",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jim-fx/plantarium.git"
  },
  "engines": {
