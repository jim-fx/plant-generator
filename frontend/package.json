{
  "name": "@plantarium/frontend",
  "version": "0.0.7",
  "description": "Tool for the procedural generation of 3D plants.",
  "main": "public/dist/bundle.js",
  "bundle": "public/dist/bundle.js",
  "scripts": {
    "dev": "rollup -c -w",
    "start": "yarn server:start",
    "build": "yarn build:client",
    "build:client": "rollup -c && mv stats.html public/stats.html",
    "heroku-postbuild": "echo nope",
    "clean": "rm -rf public/dist"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@plantarium/generator": "file:../packages/generator",
    "@plantarium/helpers": "file:../packages/helpers",
    "@plantarium/nodes": "file:../packages/nodes",
    "@plantarium/nodesystem": "file:../packages/nodesystem",
    "@plantarium/renderer": "file:../packages/renderer",
    "@plantarium/types": "file:../packages/types",
    "ogl-typescript": "^0.1.20",
    "resize-observer-polyfill": "^1.5.1",
    "settings-ui": "^1.3.2"
  }
}
