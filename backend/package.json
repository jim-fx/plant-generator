{
  "name": "@plantarium/backend",
  "version": "0.0.1",
  "module": "src/types.ts",
  "types": "src/types.ts",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "node dist/main",
    "dev": "nodemon",
    "start:dev": "ts-eager src/main.ts",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache",
    "migrate": "mikro-orm schema:create --run",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/cli": "5.6.16",
    "@mikro-orm/core": "5.6.16",
    "@mikro-orm/migrations": "5.6.16",
    "@mikro-orm/mongo-highlighter": "1.0.0",
    "@mikro-orm/mongodb": "5.6.16",
    "@mikro-orm/mysql": "5.6.16",
    "@mikro-orm/nestjs": "5.1.8",
    "@mikro-orm/postgresql": "5.6.16",
    "@mikro-orm/reflection": "5.6.16",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@mikro-orm/sqlite": "5.6.16",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/platform-fastify": "9.4.0",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/terminus": "9.2.2",
    "@nestjs/throttler": "4.0.0",
    "@plantarium/helpers": "workspace:*",
    "@plantarium/ui": "workspace:*",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "express": "4.18.2",
    "file-saver": "2.0.5",
    "helmet": "6.1.5",
    "morgan": "1.10.0",
    "octokit": "2.0.14",
    "passport": "0.6.0",
    "passport-github": "1.1.0",
    "passport-github2": "0.1.12",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.0",
    "rxjs": "7.8.0",
    "stacktrace-parser": "0.1.10",
    "svelte": "3.58.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.4.0",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@plantarium/types": "workspace:*",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.11",
    "@types/passport-github2": "^1.2.5",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.22",
    "ogl-typescript": "0.1.40",
    "prettier": "2.8.7",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-eager": "2.0.2",
    "ts-jest": "29.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "configPaths": [
      "./dist/mikro-orm.config.js",
      "./src/mikro-orm.config.ts"
    ]
  }
}
